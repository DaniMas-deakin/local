  agent any

  stages {
    stage('Build') {
      steps {
        echo 'Building...'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing...'
      }
    }
    stage('Code Quality Check') {
      steps {
        echo 'Checking Code Quality...'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploying...'
      }
    }
    stage('Approval') {
      steps {
        echo 'Approving...'
      }
    }
    stage('Deploy to Production') {
      steps {
        echo 'Deploying to Production...'
      }
    }
  }
}
    
    
